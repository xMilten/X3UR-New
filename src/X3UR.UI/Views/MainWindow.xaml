<Window x:Class="X3UR.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:X3UR.UI"
        xmlns:userSettings="clr-namespace:X3UR.UI.Views.UserControls.UserSettings"
        xmlns:visualUniverse="clr-namespace:X3UR.UI.Views.UserControls.VisualUniverse"
        mc:Ignorable="d"
        Title="X3 - Universe Ranomizer"
        Height="1030"
        Width="1916"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*" />
            <ColumnDefinition Width="7*" />
        </Grid.ColumnDefinitions>

        <userSettings:UserSettingsPanel AutomationProperties.AutomationId="UserSettingsPanel" Grid.Column="0" DataContext="{Binding SettingsVm}" />
        <visualUniverse:VisualUniversePanel Grid.Column="1" />
    </Grid>
</Window>